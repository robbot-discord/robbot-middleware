{
  "author": "Robert Smieja",
  "description": "Run different event handlers per server. Middleware for RobBot",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "template",
    "babel"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@robbot/per-server-event-handlers",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:js lint",
    "build:js": "tsc",
    "clean": "rimraf dist/",
    "lint": "eslint ./src/**/*",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint --fix ./src/**/*",
    "prettier": "prettier --write '*.{js,ts,jsx,tsx,json,md}'",
    "prepublishOnly": "npm-run-all --parallel test lint",
    "start": "ts-node src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "version": "pnpm run lint:fix && git add -A src"
  },
  "version": "0.0.6",
  "dependencies": {
    "@robbot/robbot-core": "0.0.13",
    "immer": "^7.0.9"
  },
  "devDependencies": {
    "@types/ramda": "^0.27.19",
    "eslint": "^7.10.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  }
}
