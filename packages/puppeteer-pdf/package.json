{
  "author": "Robert Smieja",
  "description": "Middleware for RobBot to save links as PDFs via Puppeteer",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "template",
    "babel"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@robbot/puppeteer-pdf",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:js lint",
    "build:js": "tsc",
    "clean": "rimraf dist/",
    "lint": "eslint ./src/**/*",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint --fix ./src/**/*",
    "prettier": "prettier --write '*.{js,ts,jsx,tsx,json,md}'",
    "prepublishOnly": "npm-run-all --parallel test lint",
    "start": "ts-node src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "version": "pnpm run lint:fix && git add -A src"
  },
  "version": "0.0.3",
  "dependencies": {
    "@robbot/robbot-core": "0.0.11",
    "puppeteer": "^5.3.0"
  },
  "devDependencies": {
    "@types/puppeteer": "^3.0.2",
    "npm-run-all": "^4.1.5"
  }
}
