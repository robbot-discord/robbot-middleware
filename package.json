{
  "author": "Robert Smieja",
  "description": "Mono-repo for RobBot Middleware",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.2",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lerna": "^3.22.1",
    "lint-staged": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "keywords": [
    "typescript",
    "nodejs",
    "template",
    "babel"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "name": "@robbot/robbot-middleware",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna run clean",
    "lint": "lerna run lint",
    "lint:staged": "lerna run lint:staged",
    "lint:fix": "lerna run lint:fix",
    "preinstall": "npx only-allow pnpm",
    "prettier": "lerna run prettier",
    "test": "lerna run test",
    "test:watch": "lerna run test:watch"
  },
  "version": "0.0.0"
}
